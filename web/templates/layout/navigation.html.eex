<%= if signed_in?(@conn) do %>
  <%= link gettext("Sign out"), to: session_path(@conn, :delete, current_user(@conn).id), method: :delete, data: [role: "sign-out"] %></strong>
<% else %>
  <%= link(gettext("Sign up"),
    to: sign_up_path(@conn, :new),
    data: [role: "sign-up"])
    %>

  <%= link(gettext("Sign in"),
    to: sign_in_path(@conn, :new),
    data: [role: "sign-in"])
    %>
<% end %>
